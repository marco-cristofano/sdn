!
!
service integrated-vtysh-config
!
interface eth0
 ip address 192.172.64.1/27
 ipv6 nd suppress-ra
!
interface eth2
 ip address 192.172.0.1/30
 ipv6 nd suppress-ra
!
interface lo
!
router ospf
 ospf router-id 192.172.64.1
 network 192.172.0.0/30 area 0.0.0.0
 network 192.172.64.0/27 area 0.0.0.0
!
ip forwarding
ipv6 forwarding
!
line vty
!
end
