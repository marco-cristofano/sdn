node n9 {
    type router
    model router
    network-config {
	hostname n9
	!
	interface eth1
	 ip address 192.172.0.13/30
	!
	interface eth0
	 ip address 192.172.0.10/30
	!
	interface eth2
	 ip address 192.168.57.103/24
	!
    }
    canvas c1
    iconcoords {461.0 322.0}
    labelcoords {464.0 365.0}
    interface-peer {eth0 n8}
    interface-peer {eth1 n16}
    interface-peer {eth2 n7}
    services {zebra OSPFv2 BGP vtysh IPForward}
    custom-config {
	custom-config-id service:zebra
	custom-command zebra
	config {
	files=('/usr/local/etc/quagga/Quagga.conf', 'quaggaboot.sh', '/usr/local/etc/quagga/vtysh.conf', )
	}
    }
    custom-config {
	custom-config-id service:zebra:/usr/local/etc/quagga/Quagga.conf
	custom-command /usr/local/etc/quagga/Quagga.conf
	config {
	interface eth0
	  ip address 192.172.0.10/30
	  
	!
	interface eth1
	  ip address 192.172.0.13/30
	  
	!
	interface eth2
	  ip address 192.168.57.103/24
	  
	!
	router ospf
	  router-id 192.172.0.10
	  network 192.172.0.8/30 area 0
	  network 192.172.0.12/30 area 0
	  no network 192.168.57.0/24 area 0
	!
	!
	! BGP configuration
	!
	! You should configure the AS number below,
	! along with this router's peers.
	!
	router bgp 9
	  bgp router-id 192.172.0.10
	  redistribute connected
	! neighbor 1.2.3.4 remote-as 555
	!
	
	}
    }
    custom-config {
	custom-config-id service:BGP
	custom-command BGP
	config {
	dirs=
	files=
	startidx=40
	cmdup=('sh quaggaboot.sh bgpd', )
	cmddown=('killall bgpd', )
	cmdval=('pidof bgpd', )
	meta=The config file for this service can be found in the Zebra service.
	starttime=
	}
    }
}

