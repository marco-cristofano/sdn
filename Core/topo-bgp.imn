node n4 {
    type router
    model router
    network-config {
	hostname n4
	!
	interface eth0
	 ip address 192.172.64.1/27
	!
	interface eth2
	 ip address 192.172.0.1/30
	!
    }
    canvas c1
    iconcoords {235.0 70.0}
    labelcoords {240.0 51.0}
    services {zebra OSPFv2 vtysh IPForward}
    interface-peer {eth2 n8}
    interface-peer {eth0 n1}
    custom-config {
	custom-config-id service:zebra
	custom-command zebra
	config {
	files=('/usr/local/etc/quagga/Quagga.conf', 'quaggaboot.sh', '/usr/local/etc/quagga/vtysh.conf', )
	}
    }
    custom-config {
	custom-config-id service:zebra:/usr/local/etc/quagga/Quagga.conf
	custom-command /usr/local/etc/quagga/Quagga.conf
	config {
	interface eth0
	  ip address 192.172.64.1/27
	!
	interface eth2
	  ip address 192.172.0.1/30
	!
	router ospf
	  router-id 192.172.64.1
	  network 192.172.64.0/27 area 1
	  network 192.172.0.0/30 area 1
	!
	
	}
    }
}

